<h2>Search Books</h2>
<form action='/books' method='GET'>
  <div>
  <label for="name">Title</label>
  <input type="text" name='title' value='<%= searchOptions.title%>'>
  </div>
  <div>
  <label for="date">Published After</label>
  <input type="date" name='publishhedAfter' value='<%= searchOptions.publisedAfter%>'>
  </div>
  <div>
  <label for="date">Published Before</label>
  <input type="date" name='publishhedBefore' value='<%= searchOptions.publisedBefore%>'>
  </div>
  <button type="submit">Search</button>
</form>
<br>
<div>
<% books.forEach(book => {  %>
  <a href="/books/<%=book.id%>">
  <img height='150' width='100' src="<%= book.coverImagePath %>" alt="img">
  </a>
<% }) %>
</div>